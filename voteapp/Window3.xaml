<Window x:Class="voteapp.Window3"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:voteapp"
        mc:Ignorable="d"
        Title="Window3" Height="450" Width="800">
    <Grid x:Name="personcenterWin" Background="{DynamicResource {x:Static SystemColors.ActiveCaptionBrushKey}}">
        <Label Content="个人中心" HorizontalAlignment="Center" Margin="0,25,0,0" VerticalAlignment="Top" FontSize="18" Height="40" Width="90"/>
        <Label Content="Welcome :" HorizontalAlignment="Left" Margin="480,65,0,0" VerticalAlignment="Top" Height="30" Width="75" RenderTransformOrigin="0.133,-0.008"/>
        <Button Content="返回" HorizontalAlignment="Left" Margin="40,25,0,0" VerticalAlignment="Top" Height="50" Width="95" FontSize="18" Background="#FFEDECEC" Padding="-1,1,1,1" SnapsToDevicePixels="True" Click="Button_Click"/>
        <ListView Margin="575,65,130,340" FontSize="18">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>

    </Grid>
</Window>
